<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.mostovyi.smartstartup.controller.CreateProgramController">
    <children>
        <TextField fx:id="programNameTextField" text="Please fill in name of the Program"/>
        <Button mnemonicParsing="false" onAction="#createProgram" text="Create"/>
        <Button mnemonicParsing="false" onAction="#cancel" text="Cancel"/>
    </children>
</VBox>
